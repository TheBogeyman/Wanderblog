doctype html
html
    head
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1.0')
        title #{title} - Wanderblog
        link(rel='stylesheet', type='text/css', href='/stylesheets/bootstrap.min.css')
        link(rel='stylesheet', type='text/css', href='/stylesheets/style.css')
        link(rel='stylesheet', type='text/css', href='/stylesheets/newstyle.css')
        link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css')
        link(rel='stylesheet', type='text/css', href='http://fonts.googleapis.com/css?family=Slabo+27px')
        link(rel='stylesheet', type='text/css', href='http://fonts.googleapis.com/css?family=Lato')
        link(rel='shortcut icon', href='/images/favicon.ico')
        script(src='/javascripts/modernizr-2.6.2.js')
        script(src='https://code.jquery.com/jquery-1.10.2.min.js')
        script(type='text/javascript', src='http://maps.google.com/maps/api/js?sensor=false&libraries=places')
        script(src='/locationpicker/src/locationpicker.jquery.js')
        link(rel='stylesheet', href='/fancybox/jquery.fancybox-1.3.4.css', type='text/css', media='screen')


    body
        include navigation
        .container.body-content
            #{error}
            block content
            hr
        include foot

        script(src='/javascripts/jquery-1.10.2.js')
        script(src='/javascripts/bootstrap.js')
        script(src='/javascripts/respond.js')
        script(type='text/javascript', src='/fancybox/jquery.fancybox-1.3.4.pack.js')
        script(type='text/javascript', src='/fancybox/jquery.easing-1.4.pack.js')


        block scripts
